{
  "name": "@rich-ui/icons",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.6.2",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm run -C playground dev",
    "build": "pnpm run -C ./packages/vue build",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write . && pnpm run -C ./packages/svg optimize",
    "release": "bumpp -r"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^3.0.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  }
}